{
    "name": "wemx/pterodactylregister",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "description": "Register Module installer for Pterodactyl",
    "homepage": "https://github.com/VertisanPRO/PterodactylRegister#readme",
    "license": "GPL-3.0-only",
    "type": "library",
    "authors": [
        {
            "name": "PadowYT2",
            "email": "me@padow.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "wemx/utils": "*",
        "laravel/prompts": "*"
    },
    "require-dev": {
        "laravel/framework": "~10.1.3",
        "guzzlehttp/guzzle": "~7.5.0"
    },
    "autoload": {
        "psr-4": {
            "Wemx\\PterodactylRegister\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wemx\\PterodactylRegister\\CommandsServiceProvider"
            ]
        }
    }
}
